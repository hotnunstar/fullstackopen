{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2022",
		"strict": true,
		"resolveJsonModule": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,

		"noImplicitAny": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
	}
}
